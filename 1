#include <iostream>

int main() {
    const int rows = 7;
    const int cols = 7;

    for (int i = 0; i < rows; ++i) {
        for (int j = 0; j < cols; ++j) {
            //
            if (j == (rows / 2) && i < (rows / 2) || (i == 0 && (j == 2 || j == 4)) || (i == 1 && j == 3) || (i == 2 && (j == 0 || j == 6))) {
                std::cout << "*";
            } else {
                std::cout << " ";
            }
        }
        std::cout << std::endl; //
    }

    return 0;
}
