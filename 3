#include <iostream>

int main() {
    const int size = 7;
    char letterY[size][size] = {
        {' ', ' ', '*', ' ', '*', ' ', ' '},
        {' ', ' ', ' ', '*', ' ', ' ', ' '},
        {' ', ' ', ' ', ' ', ' ', ' ', ' '},
        {' ', ' ', ' ', ' ', ' ', ' ', ' '},
        {' ', ' ', ' ', ' ', ' ', ' ', ' '},
        {' ', ' ', ' ', ' ', ' ', ' ', ' '},
        {' ', ' ', '*', ' ', ' ', ' ', ' '}
    };

    for (int i = 0; i < size; ++i) {
        for (int j = 0; j < size; ++j) {
            std::cout << letterY[i][j];
        }
        std::cout << std::endl; //
    }

    return 0;
}
