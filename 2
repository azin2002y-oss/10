#include <iostream>

int main() {
    const int rows = 6;
    const int cols = 7;

    for (int i = 0; i < rows; ++i) {
        for (int j = 0; j < cols; ++j) {
            //
            if (j == 0 || (i == 0 || i == 3 || i == 5) && j < 6 || (i == 1 || i == 2) && j == 6) {
                std::cout << "*";
            } else {
                std::cout << " ";
            }
        }
        std::cout << std::endl; //
    }

    return 0;
}
